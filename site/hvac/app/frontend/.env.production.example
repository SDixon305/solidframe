# Production Environment Variables
# Copy this to .env.production.local and fill in your actual values

# Backend API - Replace with your Railway backend URL
NEXT_PUBLIC_API_URL=https://your-railway-app.up.railway.app

# Supabase (Required for V2 Demo)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# n8n Webhook Base URL (production webhook)
NEXT_PUBLIC_N8N_WEBHOOK_URL=https://your-production-n8n-webhook.com
